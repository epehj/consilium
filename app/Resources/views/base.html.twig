<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}CONSILIUM{% endblock %}</title>
    {% block stylesheets %}
    <link rel="stylesheet" type="text/css" href="{{ asset('libs/patternfly/css/patternfly.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('libs/patternfly/css/patternfly-additions.css') }}">
    {% endblock %}
    {#<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}">#}
  </head>
  <body>
    {% block body %}{% endblock %}

    {% block javascripts %}
    <script src="{{ asset('libs/jquery.min.js') }}"></script>
    <script src="{{ asset('libs/bootstrap/js/bootstrap.min.js') }}"></script>
    {% block jsBeforePatternfly %}{% endblock %}
    <script src="{{ asset('libs/patternfly/js/patternfly.min.js') }}"></script>
    {% endblock %}
  </body>
</html>
